

  <div class="col-lg-12">
    <div class="panel panel-default">

      <div class="panel-heading">
        <h2>Licitação Detalhada</h2>
      </div>
    
      <br/>

    <div class="panel-body">
      <div class="form-group">
        <div class="col-lg-6">
         <h4>Data:
         <%= l @bidding.date %>
         </h4> 
        </div>
        <div class="col-lg-6">
          <h4>Orgão:
          <%= @bidding.organ %>
          </h4>
          <br />
        </div>
      </div>

      <div class="form-group">
        <div class="col-lg-6">
          <h4>Modalidade:
        <%= @bidding.modality %>
         </h4> 
        </div>
        <div class="col-lg-6">
          <h4>Objeto:
        <%= @bidding.object %>
         </h4> 
          
          <br />
        </div>
      </div>

      <div class="form-group">
        <div class="col-lg-6">
          <h4>Valor: 
           <%= @bidding.value %>
          </h4>
        </div>
        <div class="col-lg-6">
          <h4> Vistoria: 
            <%= @bidding.inspection %>
          </h4>
          <br />
        </div>
      </div>

      <div class="form-group">
        <div class="col-lg-6">
          <h4>Orçamento:
            <%= @bidding.budge %>
          </h4>
        </div>
        <div class="col-lg-6">
          <h4>Status:
            <%= @bidding.status %>
          </h4>
          <br />
        </div>

        <div class="form-group">
          <div class="col-lg-6">
            <h4>Observação:
              <%= @bidding.remark %>
            </h4>
            <br />
         
         <div class="form-group">
			<div class="col-lg-6">
				<h4>Atestados:
				
            </h4>
            <br />

         <div class="form-group">
          <div class="col-lg-6">
            <h4>Site:
              <%= @bidding.website %>
            </h4>
            <br />   
               
          </div>
        </div>
      </div>
    </div>
  </div>
  
      
  <br />
    
  
  
    <%= link_to 'Voltar', biddings_path, class: 'btn btn-primary btn-lg' %>
  
</div>


