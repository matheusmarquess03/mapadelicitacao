<p id="notice"><%= notice %></p>

<h1>Biddings</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Organ</th>
      <th>Modality</th>
      <th>Object</th>
      <th>Value</th>
      <th>Inspection</th>
      <th>Budge</th>
      <th>Remark</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @biddings.each do |bidding| %>
      <tr>
        <td><%= bidding.date %></td>
        <td><%= bidding.organ %></td>
        <td><%= bidding.modality %></td>
        <td><%= bidding.object %></td>
        <td><%= bidding.value %></td>
        <td><%= bidding.inspection %></td>
        <td><%= bidding.budge %></td>
        <td><%= bidding.remark %></td>
        <td><%= bidding.status %></td>
        <td><%= link_to 'Show', bidding %></td>
        <td><%= link_to 'Edit', edit_bidding_path(bidding) %></td>
        <td><%= link_to 'Destroy', bidding, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bidding', new_bidding_path %>
